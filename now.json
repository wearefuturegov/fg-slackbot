{
  "name": "fg-slackbot",
  "version": 2,
  "builds": [
    {
      "src": "app.rb",
      "use": "@now/ruby"
    }
  ],
  "env": {
    "SLACK_CLIENT_ID": "@slack-client-id",
    "SLACK_API_SECRET": "@slack-api-secret",
    "SLACK_REDIRECT_URI": "@slack-redirect-uri",
    "SLACK_VERIFICATION_TOKEN": "@slack-verification-token",
    "OAUTH_ACCESS_TOKEN": "@oauth-access-token"
  }
}